    "next-seo": "6.5.0",
    "react": "19.0.0-rc-1eaccd82-20240816",
    "react-dom": "19.0.0-rc-1eaccd82-20240816",
    "react-tsparticles": "2.12.2",
    "resend": "3.5.0"
